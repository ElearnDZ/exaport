<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/eportfolio/db" VERSION="20130314" COMMENT="XMLDB file for Exabis E-Portfolio"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_exaportuser" COMMENT="user in the E-portfolio" NEXT="block_exaportcate">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="true" NEXT="user_id"/>
        <FIELD NAME="user_id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" PREVIOUS="id" NEXT="description"/>
        <FIELD NAME="description" TYPE="text" NOTNULL="true" SEQUENCE="false" PREVIOUS="user_id" NEXT="persinfo_timemodified"/>
        <FIELD NAME="persinfo_timemodified" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="description" NEXT="persinfo_externaccess"/>
        <FIELD NAME="persinfo_externaccess" TYPE="int" LENGTH="1" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="persinfo_timemodified" NEXT="itemsort"/>
        <FIELD NAME="itemsort" TYPE="char" LENGTH="16" NOTNULL="true" SEQUENCE="false" PREVIOUS="persinfo_externaccess" NEXT="user_hash"/>
        <FIELD NAME="user_hash" TYPE="char" LENGTH="8" NOTNULL="true" SEQUENCE="false" PREVIOUS="itemsort" NEXT="user_hash_long"/>
        <FIELD NAME="user_hash_long" TYPE="char" LENGTH="30" NOTNULL="false" SEQUENCE="false" PREVIOUS="user_hash" NEXT="oezinstall"/>
        <FIELD NAME="oezinstall" TYPE="int" LENGTH="1" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="user_hash_long" NEXT="import_oez_tstamp"/>
        <FIELD NAME="import_oez_tstamp" TYPE="int" LENGTH="20" NOTNULL="false" UNSIGNED="false" SEQUENCE="false" COMMENT="last time oez update/import" PREVIOUS="oezinstall"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="user_id"/>
        <KEY NAME="user_id" TYPE="foreign-unique" FIELDS="user_id" REFTABLE="user" REFFIELDS="id" PREVIOUS="primary"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="block_exaportcate" COMMENT="categories for e-portfolio items" PREVIOUS="block_exaportuser" NEXT="block_exaportitem">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="true" COMMENT="id of the table, please edit me" NEXT="pid"/>
        <FIELD NAME="pid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" COMMENT="parent id" PREVIOUS="id" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" COMMENT="user id" PREVIOUS="pid" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="name" PREVIOUS="userid" NEXT="timemodified"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" COMMENT="time modified" PREVIOUS="name" NEXT="courseid"/>
        <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="timemodified" NEXT="isoez"/>
        <FIELD NAME="isoez" TYPE="int" LENGTH="1" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="courseid" NEXT="description"/>
        <FIELD NAME="description" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="oez" PREVIOUS="isoez" NEXT="subjid"/>
        <FIELD NAME="subjid" TYPE="int" LENGTH="20" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" COMMENT="subject id" PREVIOUS="description" NEXT="topicid"/>
        <FIELD NAME="topicid" TYPE="int" LENGTH="20" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" COMMENT="topic id" PREVIOUS="subjid" NEXT="source"/>
        <FIELD NAME="source" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="topicid" NEXT="sourceid"/>
        <FIELD NAME="sourceid" TYPE="int" LENGTH="20" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="source" NEXT="parent_ids"/>
        <FIELD NAME="parent_ids" TYPE="char" LENGTH="200" NOTNULL="false" SEQUENCE="false" COMMENT="list of parent category ids" PREVIOUS="sourceid" NEXT="parent_titles"/>
        <FIELD NAME="parent_titles" TYPE="text" LENGTH="medium" NOTNULL="false" SEQUENCE="false" COMMENT="list of parent categories, names" PREVIOUS="parent_ids" NEXT="stid"/>
        <FIELD NAME="stid" TYPE="int" LENGTH="20" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" COMMENT="schooltypeid" PREVIOUS="parent_titles" NEXT="sourcemod"/>
        <FIELD NAME="sourcemod" TYPE="int" LENGTH="1" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" COMMENT="modus of category exacomp schooltype=3, subject=5, topic=7" PREVIOUS="stid"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="id" TYPE="primary" FIELDS="id" COMMENT="primary key for bookmark categories" NEXT="userid"/>
        <KEY NAME="userid" TYPE="foreign" FIELDS="userid" REFTABLE="user" REFFIELDS="id" PREVIOUS="id" NEXT="pid"/>
        <KEY NAME="pid" TYPE="foreign" FIELDS="pid" REFTABLE="block_exaportcate" REFFIELDS="id" PREVIOUS="userid" NEXT="courseid"/>
        <KEY NAME="courseid" TYPE="foreign" FIELDS="courseid" REFTABLE="course" REFFIELDS="id" PREVIOUS="pid"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="block_exaportitem" COMMENT="User items" PREVIOUS="block_exaportcate" NEXT="block_exaportitemshar">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="true" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="id" NEXT="type"/>
        <FIELD NAME="type" TYPE="char" LENGTH="8" NOTNULL="true" DEFAULT="note" SEQUENCE="false" PREVIOUS="userid" NEXT="categoryid"/>
        <FIELD NAME="categoryid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="type" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" PREVIOUS="categoryid" NEXT="url"/>
        <FIELD NAME="url" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" PREVIOUS="name" NEXT="intro"/>
        <FIELD NAME="intro" TYPE="text" NOTNULL="true" SEQUENCE="false" PREVIOUS="url" NEXT="attachment"/>
        <FIELD NAME="attachment" TYPE="char" LENGTH="100" NOTNULL="true" SEQUENCE="false" PREVIOUS="intro" NEXT="timemodified"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="attachment" NEXT="courseid"/>
        <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="timemodified" NEXT="shareall"/>
        <FIELD NAME="shareall" TYPE="int" LENGTH="3" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="courseid" NEXT="externaccess"/>
        <FIELD NAME="externaccess" TYPE="int" LENGTH="3" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="shareall" NEXT="externcomment"/>
        <FIELD NAME="externcomment" TYPE="int" LENGTH="3" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="externaccess" NEXT="sortorder"/>
        <FIELD NAME="sortorder" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="false" SEQUENCE="false" PREVIOUS="externcomment" NEXT="isoez"/>
        <FIELD NAME="isoez" TYPE="int" LENGTH="1" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="sortorder" NEXT="fileurl"/>
        <FIELD NAME="fileurl" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" PREVIOUS="isoez" NEXT="beispiel_url"/>
        <FIELD NAME="beispiel_url" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" PREVIOUS="fileurl" NEXT="exampid"/>
        <FIELD NAME="exampid" TYPE="int" LENGTH="20" NOTNULL="false" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" COMMENT="if oez, exampleid of source example from exacomp" PREVIOUS="beispiel_url" NEXT="langid"/>
        <FIELD NAME="langid" TYPE="int" LENGTH="20" NOTNULL="false" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" COMMENT="desp" PREVIOUS="exampid" NEXT="beispiel_angabe"/>
        <FIELD NAME="beispiel_angabe" TYPE="text" NOTNULL="false" SEQUENCE="false" PREVIOUS="langid" NEXT="source"/>
        <FIELD NAME="source" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="beispiel_angabe" NEXT="sourceid"/>
        <FIELD NAME="sourceid" TYPE="int" LENGTH="20" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="source" NEXT="iseditable"/>
        <FIELD NAME="iseditable" TYPE="int" LENGTH="1" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="sourceid"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="userid"/>
        <KEY NAME="userid" TYPE="foreign" FIELDS="userid" REFTABLE="user" REFFIELDS="id" PREVIOUS="primary" NEXT="courseid"/>
        <KEY NAME="courseid" TYPE="foreign" FIELDS="courseid" REFTABLE="course" REFFIELDS="id" PREVIOUS="userid" NEXT="categoryid"/>
        <KEY NAME="categoryid" TYPE="foreign" FIELDS="categoryid" REFTABLE="block_exaportcate" REFFIELDS="id" PREVIOUS="courseid"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="block_exaportitemshar" COMMENT="block_exaportitemshar table retrofitted from MySQL" PREVIOUS="block_exaportitem" NEXT="block_exaportitemcomm">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="true" NEXT="itemid"/>
        <FIELD NAME="itemid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="id" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="itemid" NEXT="original"/>
        <FIELD NAME="original" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="userid" NEXT="courseid"/>
        <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="original"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="userid"/>
        <KEY NAME="userid" TYPE="foreign" FIELDS="userid" REFTABLE="user" REFFIELDS="id" PREVIOUS="primary" NEXT="courseid"/>
        <KEY NAME="courseid" TYPE="foreign" FIELDS="courseid" REFTABLE="course" REFFIELDS="id" PREVIOUS="userid" NEXT="itemid"/>
        <KEY NAME="itemid" TYPE="foreign" FIELDS="itemid" REFTABLE="block_exaportitem" REFFIELDS="id" PREVIOUS="courseid" NEXT="original"/>
        <KEY NAME="original" TYPE="foreign" FIELDS="original" REFTABLE="user" REFFIELDS="id" PREVIOUS="itemid"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="block_exaportitemcomm" COMMENT="comments for the items" PREVIOUS="block_exaportitemshar" NEXT="block_exaportview">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="true" NEXT="itemid"/>
        <FIELD NAME="itemid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="id" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="itemid" NEXT="entry"/>
        <FIELD NAME="entry" TYPE="text" NOTNULL="true" SEQUENCE="false" PREVIOUS="userid" NEXT="timemodified"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="entry"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="userid"/>
        <KEY NAME="userid" TYPE="foreign" FIELDS="userid" REFTABLE="user" REFFIELDS="id" PREVIOUS="primary" NEXT="itemid"/>
        <KEY NAME="itemid" TYPE="foreign" FIELDS="itemid" REFTABLE="block_exaportitem" REFFIELDS="id" PREVIOUS="userid"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="block_exaportview" COMMENT="views" PREVIOUS="block_exaportitemcomm" NEXT="block_exaportviewblock">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="true" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="false" SEQUENCE="false" PREVIOUS="id" NEXT="name"/>
        <FIELD NAME="name" TYPE="text" NOTNULL="false" SEQUENCE="false" PREVIOUS="userid" NEXT="description"/>
        <FIELD NAME="description" TYPE="text" NOTNULL="false" SEQUENCE="false" PREVIOUS="name" NEXT="timemodified"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="false" SEQUENCE="false" PREVIOUS="description" NEXT="shareall"/>
        <FIELD NAME="shareall" TYPE="int" LENGTH="3" NOTNULL="false" UNSIGNED="false" SEQUENCE="false" PREVIOUS="timemodified" NEXT="externaccess"/>
        <FIELD NAME="externaccess" TYPE="int" LENGTH="3" NOTNULL="false" UNSIGNED="false" SEQUENCE="false" PREVIOUS="shareall" NEXT="externcomment"/>
        <FIELD NAME="externcomment" TYPE="int" LENGTH="3" NOTNULL="false" UNSIGNED="false" SEQUENCE="false" PREVIOUS="externaccess" NEXT="hash"/>
        <FIELD NAME="hash" TYPE="char" LENGTH="8" NOTNULL="false" SEQUENCE="false" PREVIOUS="externcomment" NEXT="langid"/>
        <FIELD NAME="langid" TYPE="int" LENGTH="20" NOTNULL="false" UNSIGNED="false" SEQUENCE="false" COMMENT="desp language id" PREVIOUS="hash" NEXT="layout"/>
				<FIELD NAME="layout" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="false" SEQUENCE="false" DEFAULT="2" PREVIOUS="langid"/>        
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="userid"/>
        <KEY NAME="userid" TYPE="foreign" FIELDS="userid" REFTABLE="user" REFFIELDS="id" PREVIOUS="primary"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="hash" UNIQUE="true" FIELDS="hash"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="block_exaportviewblock" COMMENT="Blocks on the view" PREVIOUS="block_exaportview" NEXT="block_exaportviewshar">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="true" NEXT="viewid"/>
        <FIELD NAME="viewid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="false" SEQUENCE="false" PREVIOUS="id" NEXT="positionx"/>
        <FIELD NAME="positionx" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="false" SEQUENCE="false" PREVIOUS="viewid" NEXT="positiony"/>
        <FIELD NAME="positiony" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="false" SEQUENCE="false" PREVIOUS="positionx" NEXT="type"/>
        <FIELD NAME="type" TYPE="text" NOTNULL="false" SEQUENCE="false" PREVIOUS="positiony" NEXT="itemid"/>
        <FIELD NAME="itemid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="false" SEQUENCE="false" PREVIOUS="type" NEXT="text"/>
        <FIELD NAME="text" TYPE="text" NOTNULL="false" SEQUENCE="false" PREVIOUS="itemid" NEXT="block_title"/>
        <FIELD NAME="block_title" TYPE="text" NOTNULL="false" SEQUENCE="false" PREVIOUS="text" NEXT="firstname"/>
        <FIELD NAME="firstname" TYPE="char" LENGTH="150" NOTNULL="false" SEQUENCE="false" PREVIOUS="block_title" NEXT="lastname"/>
        <FIELD NAME="lastname" TYPE="char" LENGTH="150" NOTNULL="false" SEQUENCE="false" PREVIOUS="firstname" NEXT="email"/>
        <FIELD NAME="email" TYPE="char" LENGTH="150" NOTNULL="false" SEQUENCE="false" PREVIOUS="lastname" NEXT="picture"/>
        <FIELD NAME="picture" TYPE="char" LENGTH="250" NOTNULL="false" SEQUENCE="false" PREVIOUS="email" NEXT="contentmedia"/>
        <FIELD NAME="contentmedia" TYPE="text" NOTNULL="false" SEQUENCE="false" PREVIOUS="picture" NEXT="width"/>
        <FIELD NAME="width" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="false" SEQUENCE="false" PREVIOUS="contentmedia" NEXT="height"/>
        <FIELD NAME="height" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="false" SEQUENCE="false" PREVIOUS="width"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="view"/>
        <KEY NAME="view" TYPE="foreign" FIELDS="viewid" REFTABLE="block_exaportview" REFFIELDS="id" PREVIOUS="primary" NEXT="item"/>
        <KEY NAME="item" TYPE="foreign" FIELDS="itemid" REFTABLE="block_exaportitem" REFFIELDS="id" PREVIOUS="view"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="block_exaportviewshar" COMMENT="information to which users the view is shared" PREVIOUS="block_exaportviewblock">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="true" NEXT="viewid"/>
        <FIELD NAME="viewid" TYPE="int" LENGTH="20" NOTNULL="false" UNSIGNED="false" SEQUENCE="false" PREVIOUS="id" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="20" NOTNULL="false" UNSIGNED="false" SEQUENCE="false" PREVIOUS="viewid"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="view"/>
        <KEY NAME="view" TYPE="foreign" FIELDS="viewid" REFTABLE="block_exaportview" REFFIELDS="id" PREVIOUS="primary" NEXT="user"/>
        <KEY NAME="user" TYPE="foreign" FIELDS="userid" REFTABLE="user" REFFIELDS="id" PREVIOUS="view"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>